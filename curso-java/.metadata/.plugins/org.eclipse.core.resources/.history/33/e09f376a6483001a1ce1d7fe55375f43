package fundamentos;

public class Temperatura {

	public static void main(String[] args) {
		
	  int f = -32;
	  int v1 = 5;
	  double v2 = 9.0;
	  String a = "°F";
	  String b = "°C";
	  int res;
	  
	  
	  res =  f * (v1/v2);
	  
	  System.out.println(f+a);
	  
	  System.out.println(res+b);
	  
	  
	  
	  
	  
		
	}
}
